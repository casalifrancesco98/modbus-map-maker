// Auto-generated by modbus-map-maker
mapping = [
{% for e in entries -%}
  {
    "device": "{{ e.device }}",
    "name": "{{ e.name }}",
    "address": {{ e.address }},
    "dtype": "{{ e.dtype }}",
    "scale": {{ e.scale }},
    "offset": {{ e.offset }},
    "unit": {{ ('"%s"' % e.unit) if e.unit else 'null' }},
    "rw": "{{ e.rw }}",
    "function": "{{ e.function }}",
    "byte_order": "{{ e.byte_order }}",
    "word_order": "{{ e.word_order }}",
    "description": {{ ('"%s"' % e.description) if e.description else 'null' }}
  },
{% endfor -%}
]
